{
    "name": "Save Tweets",
    "description": "Save Tweets to Photos",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":[
      "contextMenus","tabs","storage","scripting",
      "activeTab","webRequest"],
    "host_permissions": ["http://localhost:5000/*"],
    "background": {
    "service_worker": "background.js"
  },
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts":[{
      "matches":["<all_urls>"],
      "js":["content.js"]
      }]
  }
